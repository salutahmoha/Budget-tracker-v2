generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Budget {
  title String @id
  quantity String? @map("quantity")
  price String? @map("price")

  @@map("budget_tracker")
}
